# Enfiy Code Environment Variables
# Copy this file to .env and set your API keys

# Google OAuth Configuration (Required for Google services)
ENFIY_GOOGLE_OAUTH_CLIENT_ID=your_google_oauth_client_id
ENFIY_GOOGLE_OAUTH_CLIENT_SECRET=your_google_oauth_client_secret

# API Keys for AI Providers
GEMINI_API_KEY=your_gemini_api_key
GOOGLE_API_KEY=your_google_api_key
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
MISTRAL_API_KEY=your_mistral_api_key
HUGGINGFACE_API_KEY=your_huggingface_api_key

# Optional: Custom API endpoints
CODE_ASSIST_ENDPOINT=https://generativelanguage.googleapis.com

# Optional: Ollama configuration
OLLAMA_HOST=http://localhost:11434