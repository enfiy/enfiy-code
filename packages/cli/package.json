{
  "name": "@enfiy/enfiy-code",
  "version": "0.1.0",
  "description": "Enfiy Code",
  "repository": "enfiy-ecosystem/enfiy-code",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "enfiy-code": "dist/index.js",
    "enfiy": "dist/index.js"
  },
  "scripts": {
    "build": "node ../../scripts/build_package.js",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "start": "node dist/index.js",
    "debug": "node --inspect-brk dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:ci": "eslint . --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prerelease:version": "node ../../scripts/bind_package_version.js",
    "prerelease:deps": "node ../../scripts/bind_package_dependencies.js",
    "prepack": "npm run build",
    "prepublishOnly": "node ../../scripts/prepublish.js"
  },
  "files": [
    "dist"
  ],
  "config": {
    "sandboxImageUri": "enfiy-code-sandbox"
  },
  "dependencies": {
    "@enfiy/cli": "file:../cli",
    "@enfiy/core": "file:../core",
    "@types/update-notifier": "^6.0.8",
    "command-exists": "^1.2.9",
    "diff": "^8.0.2",
    "dotenv": "^17.2.0",
    "gaxios": "^7.1.1",
    "glob": "^11.0.3",
    "ink": "^6.0.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "lowlight": "^3.3.0",
    "mime-types": "^3.0.1",
    "open": "^10.1.2",
    "react": "^19.1.0",
    "read-package-up": "^11.0.0",
    "shell-quote": "^1.8.3",
    "string-width": "^7.2.0",
    "strip-ansi": "^7.1.0",
    "strip-json-comments": "^5.0.2",
    "update-notifier": "^7.3.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/command-exists": "^1.2.3",
    "@types/diff": "^8.0.0",
    "@types/dotenv": "^8.2.3",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/shell-quote": "^1.7.5",
    "@types/yargs": "^17.0.33",
    "ink-testing-library": "^4.0.0",
    "jsdom": "^26.1.0",
    "jsdom-global": "^3.0.2",
    "memfs": "^4.17.2",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  }
}
