{
  "name": "@enfiy/core",
  "version": "0.1.8",
  "description": "Enfiy Code Server",
  "repository": "enfiy/enfiy-code",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "node ../../scripts/build_package.js",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:ci": "eslint . --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prerelease:version": "node ../../scripts/bind_package_version.js",
    "prerelease:deps": "node ../../scripts/bind_package_dependencies.js",
    "prepack": "npm run build",
    "prepublishOnly": "node ../../scripts/prepublish.js"
  },
  "files": [
    "dist",
    "README.md",
    "../../LICENSE"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@google/genai": "^1.9.0",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.203.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.203.0",
    "@opentelemetry/instrumentation-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "diff": "^8.0.2",
    "dotenv": "^17.2.0",
    "gaxios": "^7.1.1",
    "glob": "^11.0.3",
    "google-auth-library": "^10.1.0",
    "html-to-text": "^9.0.5",
    "ignore": "^7.0.5",
    "mime-types": "^3.0.1",
    "open": "^10.1.2",
    "shell-quote": "^1.8.3",
    "simple-git": "^3.28.0",
    "strip-ansi": "^7.1.0",
    "undici": "^7.11.0"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/micromatch": "^4.0.9",
    "@types/mime-types": "^3.0.1",
    "@types/ws": "^8.18.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
